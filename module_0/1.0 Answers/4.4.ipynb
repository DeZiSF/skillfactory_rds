   "source": [
    "def get_median(x):\n",
    "    x.sort()\n",
    "    length = len(x)    \n",
    "    if length % 2 != 0:\n",
    "        return x[length//2]\n",
    "    else:\n",
    "        return (x[length//2] + x[length//2 - 1]) / 2"
   ]
  },
